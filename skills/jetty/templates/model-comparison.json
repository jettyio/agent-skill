{
  "init_params": {
    "prompt": "Explain quantum computing in simple terms",
    "model_a": "gpt-4o",
    "model_b": "claude-3-5-sonnet-20241022",
    "judge_model": "gpt-4o",
    "temperature": 0.7
  },
  "step_configs": {
    "response_a": {
      "activity": "litellm_chat",
      "model_path": "init_params.model_a",
      "user_prompt_path": "init_params.prompt",
      "temperature_path": "init_params.temperature"
    },
    "response_b": {
      "activity": "litellm_chat",
      "model_path": "init_params.model_b",
      "user_prompt_path": "init_params.prompt",
      "temperature_path": "init_params.temperature"
    },
    "compare": {
      "activity": "simple_judge",
      "items": [
        "response_a.outputs.text",
        "response_b.outputs.text"
      ],
      "instruction": "Compare these two responses for clarity, accuracy, and helpfulness. Consider which one would be more useful for someone learning about this topic.",
      "judge_type": "categorical",
      "categories": ["first_better", "second_better", "tie"],
      "with_explanation": true,
      "model_path": "init_params.judge_model"
    }
  },
  "steps": ["response_a", "response_b", "compare"]
}
